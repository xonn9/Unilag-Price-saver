{
  "version": 2,
  "builds": [
    { "src": "frontend/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/login", "dest": "frontend/login.html" },
    { "src": "/dashboard", "dest": "frontend/user-dashboard.html" },
    { "src": "/admin", "dest": "frontend/admin-dashboard.html" },
    { "src": "/(.*)", "dest": "frontend/$1" }
  ]
}
